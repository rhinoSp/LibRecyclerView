<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwipeItemLayout">
        <attr name="sil_swipeDirection" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="sil_bottomMode" format="enum">
            <enum name="pullOut" value="0" />
            <enum name="layDown" value="1" />
        </attr>
        <attr name="sil_springDistance" format="dimension" />
        <attr name="sil_swipeEnable" format="boolean" />
        <attr name="sil_swipeAlphaEnable" format="boolean" />
    </declare-styleable>
</resources>